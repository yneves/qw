{
  "name": "qw-evad-server-<%= env.name %>",
  "cwd": "/home/scripts/qw/evad-<%= env.name %>",
  "args": [],
  "script": "server/server.js",
  "node_args": [],
  "log_date_format": "YYYY-MM-DD HH:mm Z",
  "error_file": "logs/server-stderr.log",
  "out_file": "logs/server-stdout.log",
  "pid_file": "server/server.pid",
  "min_uptime": "200s",
  "max_restarts": 10,
  "max_memory_restart": "100M",
  "cron_restart": "1 0 * * *",
  "merge_logs": true,
  "exec_interpreter": "node",
  "exec_mode": "fork",
  "autorestart": true,
  "vizion": false,
  "env": {
    "NODE_ENV": "<%= env.name %>"
  }
}
